{
  "name": "Get pages source",
  "version": "0.3.1",
  "manifest_version": 2,
  "description": "Get pages source from a popup",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage", 
    "cookies",
    "https://familysearch.org/"
  ]
}